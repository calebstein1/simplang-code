{
    "comments": {
        "lineComment": "; ",
    },
    "brackets": [
        ["(", ")"],
        ["[", "]"],
    ],
    "autoClosingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["\"", "\""],
    ],
    "surroundingPairs": [
        ["[", "]"],
        ["\"", "\""],
    ],
    "indentationRules": {
        "increaseIndentPattern": "^ *(begin_loop|for_each|if_equal|if_not_equal|if_less|if_less_or_equal|if_greater|if_greater_or_equal|if_in_range|if_not_in_range).*$",
        "decreaseIndentPattern": "^ *(end_if|end_loop|end_loop_equal|end_loop_not_equal|end_loop_if_less|end_loop_if_less_or_equal|end_loop_if_greater|end_loop_if_greater_or_equal).*$"
    }
}